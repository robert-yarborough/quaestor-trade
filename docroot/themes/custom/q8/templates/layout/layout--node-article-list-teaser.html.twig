{%
  set classes = [
  'layout',
  'layout--onecol',
]
%}
{% if content %}
  <div{{ attributes.addClass(classes) }}>
    <div {{ region_attributes.content.addClass('layout__region', 'layout__region--content') }}>
      {#{{ content.content.node_post_date|render|time_diff }}#}
      {#|time_diff#}
      {#{{'2014-10-11 12:54:37'|ago}}#}
      {{ content.content }}
    </div>
  </div>
{% endif %}
